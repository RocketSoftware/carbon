{
  "name": "@rocketsoftware/elements",
  "description": "A collection of design elements in code for the IBM Design Language",
  "version": "0.2.8",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": "https://github.com/RocketSoftware/carbon/tree/master/packages/elements",
  "bugs": "https://github.com/RocketSoftware/carbon/issues",
  "files": [
    "es",
    "lib",
    "scss",
    "src",
    "umd"
  ],
  "keywords": [
    "rocketsoftware",
    "elements",
    "carbon",
    "carbon-elements",
    "carbon-design-system",
    "components",
    "react"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && bundler bundle src/index.js --name CarbonElements && node tasks/build.js && bundler check \"scss/*.scss\" && bundler sassdoc \"scss/**/*.scss\" -i \"**/{_inlined,vendor}/**\"",
    "clean": "rimraf es lib umd && node tasks/clean.js"
  },
  "dependencies": {
    "@carbon/import-once": "10.3.0",
    "@rocketsoftware/colors": "^0.2.8",
    "@rocketsoftware/grid": "^0.2.8",
    "@rocketsoftware/icons": "^0.2.8",
    "@rocketsoftware/layout": "^0.2.8",
    "@rocketsoftware/motion": "^0.2.8",
    "@rocketsoftware/themes": "^0.2.8",
    "@rocketsoftware/type": "^0.2.8"
  },
  "devDependencies": {
    "@carbon/bundler": "10.4.0",
    "fs-extra": "^7.0.1",
    "klaw-sync": "^6.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2"
  }
}
